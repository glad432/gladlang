PRINTLN "--- FOR Loop Tester ---"

LET my_list = ["apple", "banana", "STOP", "cherry", "date"]
PRINTLN "Iterating over list: " + STR(my_list)

PRINTLN "--- Test 1: Full Iteration ---"
FOR item IN my_list
  PRINTLN "Item: " + item
ENDFOR

PRINTLN "--- Test 2: FOR loop with BREAK ---"
PRINTLN "Looping until we find 'STOP'..."
FOR item IN my_list
  IF item == "STOP" THEN
    PRINTLN "Found STOP, breaking loop."
    BREAK
  ENDIF
  PRINTLN "Item: " + item
ENDFOR

PRINTLN "--- Test 3: FOR loop with CONTINUE ---"
PRINTLN "Looping, but skipping 'STOP'..."
FOR item IN my_list
  IF item == "STOP" THEN
    PRINTLN "(Skipping an item)"
    CONTINUE
  ENDIF
  PRINTLN "Item: " + item
ENDFOR