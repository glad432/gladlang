PRINTLN "--- List Tester ---"

LET my_list = [1, "hello", 2 * 3, TRUE]

PRINTLN "Initial list: " + STR(my_list)

PRINTLN "--- List Access ---"
PRINTLN "Element at index 0: " + STR(my_list[0])
PRINTLN "Element at index 1: " + STR(my_list[1])
PRINTLN "Element at index 2: " + STR(my_list[2])

PRINTLN "--- List Set ---"
PRINTLN "Setting my_list[1] = \"world\""
LET my_list[1] = "world"
PRINTLN "New list: " + STR(my_list)
PRINTLN "New element at index 1: " + STR(my_list[1])

PRINTLN "--- List Concatenation ---"
LET other_list = [FALSE, 100]
LET combined_list = my_list + other_list
PRINTLN "Combined list: " + STR(combined_list)

PRINTLN "--- Out of Bounds Error Test ---"
PRINTLN "Attempting to access my_list[99]..."

TRY
    LET bad_access = my_list[99] # This line should crash
CATCH error
    PRINTLN error
ENDTRY

PRINTLN "--- List Repetition ---"
LET zeros = [0] * 5
PRINTLN "Zeros: " + STR(zeros)
# Expected: [0, 0, 0, 0, 0]