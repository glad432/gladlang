PRINTLN "--- Operator Precedence Tester ---"

LET math_result = 1 + 2 * 3 # Should be 1 + 6 = 7
PRINTLN "1 + 2 * 3 = " + math_result

LET math_result = (1 + 2) * 3 # Should be 3 * 3 = 9
PRINTLN "(1 + 2) * 3 = " + math_result

LET math_result = 10 - 4 / 2 # Should be 10 - 2 = 8
PRINTLN "10 - 4 / 2 = " + math_result

PRINTLN "--- String Comparison Tester ---"
LET s1 = "apple"
LET s2 = "banana"

PRINTLN "s1 < s2 (TRUE): " + (s1 < s2)
PRINTLN "s1 > s2 (FALSE): " + (s1 > s2)
PRINTLN "s1 == \"apple\" (TRUE): " + (s1 == "apple")
PRINTLN "s1 >= \"apple\" (TRUE): " + (s1 >= "apple")
PRINTLN "s2 <= \"banana\" (TRUE): " + (s2 <= "banana")