PRINTLN "--- Test: Nested Try-Catch & Rethrowing ---"

TRY
    PRINTLN "Outer TRY started..."
    
    TRY
        PRINTLN "  Inner TRY started..."
        THROW "Error from Inner"
    CATCH innerErr
        PRINTLN "  Inner CATCH caught: " + innerErr
        PRINTLN "  Rethrowing error to Outer..."
        THROW innerErr
    FINALLY
        PRINTLN "  Inner FINALLY executed."
    ENDTRY

CATCH outerErr
    PRINTLN "Outer CATCH caught rethrown error: " + outerErr
FINALLY
    PRINTLN "Outer FINALLY executed."
ENDTRY