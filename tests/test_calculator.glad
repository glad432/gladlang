PRINTLN "--- Calculator ---"
PRINT "Enter first number:"
LET a = INT(INPUT())

PRINT "Enter second number:"
LET b = INT(INPUT())

PRINTLN "-----------------------------"
PRINTLN "a = " + a
PRINTLN "b = " + b

PRINTLN "a + b = " + (a + b)
PRINTLN "a - b = " + (a - b)
PRINTLN "a * b = " + (a * b)
PRINTLN "a / b = " + (a / b)