PRINTLN "--- Logic & Comparison Operator Tester ---"

LET a = 10
LET b = 20
LET t = TRUE
LET f = FALSE

PRINTLN "--- Comparisons ---"
PRINTLN "a != b (TRUE): " + (a != b)
PRINTLN "a != 10 (FALSE): " + (a != 10)
PRINTLN "a <= 10 (TRUE): " + (a <= 10)
PRINTLN "a <= 9 (FALSE): " + (a <= 9)
PRINTLN "a >= 10 (TRUE): " + (a >= 10)
PRINTLN "a >= 11 (FALSE): " + (a >= 11)
PRINTLN "a < b (TRUE): " + (a < b)
PRINTLN "b > a (TRUE): " + (b > a)

PRINTLN "--- Logic ---"
PRINTLN "t AND t (TRUE): " + (t AND t)
PRINTLN "t AND f (FALSE): " + (t AND f)
PRINTLN "f AND f (FALSE): " + (f AND f)

PRINTLN "t OR t (TRUE): " + (t OR t)
# The typo 'E ' has been removed from this line
PRINTLN "t OR f (TRUE): " + (t OR f)
PRINTLN "f OR f (FALSE): " + (f OR f)

# The illegal ':' has been removed from this line
PRINTLN "NOT t (FALSE): " + (NOT t)
PRINTLN "NOT f (TRUE): " + (NOT f)

PRINTLN "--- Complex Logic ---"
# This tests operator precedence (AND before OR)
# (f OR t) AND (t AND t) => (TRUE) AND (TRUE) => TRUE
PRINTLN "f OR t AND t (TRUE): " + (f OR t AND t)

# (a < b) AND (b > 15) => TRUE AND TRUE => TRUE
PRINTLN "(a < b) AND (b > 15) (TRUE): " + ((a < b) AND (b > 15))