PRINTLN "--- Chained Call Tester ---"

CLASS Engine
  DEF start()
    PRINTLN "Engine starts. Vroom!"
    RETURN "Engine-OK"
  ENDDEF
ENDCLASS

CLASS Car
  DEF Car()
    # A car "has a" engine
    THIS.engine = NEW Engine()
  ENDDEF
  
  DEF start_car()
    PRINTLN "Turning key..."
    # This is a chained call: THIS.engine.start()
    LET status = THIS.engine.start()
    PRINTLN "Car start status: " + status
  ENDDEF
ENDCLASS

PRINTLN "Creating car..."
LET my_car = NEW Car()

PRINTLN "--- Starting Car ---"
my_car.start_car() # This will test the chained call